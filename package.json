  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:devbuild": "cross-env NODE_ENV=dev nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:preprod": "cross-env NODE_ENV=dev node dist/main.js",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:swc": "cross-env NODE_ENV=dev nest start -b swc -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
